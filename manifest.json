{
  "manifest_version": 3,
  "name": "Bookmarks to Tab Groups",
  "version": "1.0",
  "description": "Convert Chrome bookmarks to Tab Groups rules",
  "permissions": [
    "bookmarks",
    "downloads"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
} 